<link href="<%=request.getContextPath()%>/javascripts/bootstrap_datepicker/bootstrap-datetimepicker.min.css" rel="stylesheet">

<script type="text/javascript" src="<%=request.getContextPath()%>/jquery/jquery.min.js"></script>
